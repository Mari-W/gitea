{{template "base/head" .}}
<div class="page-content user link-account">
	<div class="sixteen wide aligned column">
		<iframe style="border: 0; overflow: hidden; width: 100%;  height: 350px" scrolling="no"
						src="https://course.contravariant.net/embed"></iframe>
		<div class="actions ml-5">
			<a class="ui green basic ok button" href="{{AppSubUrl}}/user/oauth2/rz">
				<i class="save icon"></i>
				Continue
			</a>
			<a class="ui red basic ok button link-action" href data-url="{{AppSubUrl}}/user/logout"
				 data-redirect="https://course.contravariant.net/logout?redirect=https://git.contravariant.net">
				<i class="cancel icon"></i>
				Cancel
			</a>
		</div>
	</div>
</div>


{{template "base/footer" .}}
